<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
	<class name="com.art.schema.SerialNoSchema" table="SERIALNO">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="identity" />
		</id>
		<property name="notype"		type="java.lang.String" 		column="notype" />
		<property name="name"		type="java.lang.String" 		column="name" />
		<property name="nolength"	type="java.lang.Integer" 	column="nolength" />
		<property name="prefix"		type="java.lang.String" 		column="prefix" />
		<property name="maxno"		type="java.lang.Integer" 	column="maxno" />
		<property name="remark"		type="java.lang.String" 		column="remark"  />
	</class>
</hibernate-mapping>