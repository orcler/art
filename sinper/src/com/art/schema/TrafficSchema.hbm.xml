<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
	<class name="com.art.schema.TrafficSchema" table="TRAFFIC">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="identity" />
		</id>
		<property name="serialNo"	type="java.lang.String" 	column="serialNo" />
		<property name="EngineNo"	type="java.lang.String" 	column="EngineNo" />
		<property name="VIN"			type="java.lang.String" 	column="VIN" />
		<property name="state"			type="java.lang.String" 	column="state" />
		<property name="paymode"			type="java.lang.String" 	column="paymode" />
		<property name="uwflag"		type="java.lang.String" 	column="uwflag" />
		<property name="model"		type="java.lang.String" 	column="model" />
		<property name="cost"			type="java.lang.Double" column="cost"  />
		<property name="cert"			type="java.lang.String" 	column="cert" />
		<property name="mileage"	type="java.lang.Double" column="mileage"	/>
		<property name="color"		type="java.lang.String" 	column="color" />
		<property name="attn"			type="java.lang.String" 	column="attn" />
		<property name="phone"		type="java.lang.String" 	column="phone" />
		<property name="comcode"	type="java.lang.String" 	column="comcode" />
		<property name="indate"		type="java.sql.Date" 		column="indate" />
		<property name="intime"		type="java.lang.String" 	column="intime" />
		<property name="outdate"	type="java.sql.Date" 		column="outdate" />
		<property name="outtime"		type="java.lang.String" 	column="outtime" />
		<property name="grpname"		type="java.lang.String" 	column="grpname" />
		<property name="remark"		type="java.lang.String" 	column="remark" />
		<property name="info1"		type="java.lang.String" 	column="info1"	 />
		<property name="info2"		type="java.lang.String" 	column="info2"	 />
		<property name="info3"		type="java.lang.String" 	column="info3"	 />

	</class>
</hibernate-mapping>